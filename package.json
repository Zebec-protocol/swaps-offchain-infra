        "pg": "^8.8.0",
        "prom-client": "^14.1.0",
        "rate-limiter-flexible": "^2.4.1",
        "siwe": "^1.1.6",
        "socket.io": "^4.5.4",
        "tiny-typed-emitter": "^2.1.0",
