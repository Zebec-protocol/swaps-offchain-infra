        "generate:lib": "npx nx generate @nrwl/js:library $1 --importPath @mycelium-ethereum/$1",
        "generate:node": "npx nx generate @nrwl/node:app $1 --importPath @mycelium-ethereum/$1"
    },
    "private": true,
